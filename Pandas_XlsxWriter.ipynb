    "from openpyxl import Workbook\n",
    "\n",
    "wb = Workbook()\n",
    "ws = wb.active\n",
    "wb.title = 'chart'\n",
    "\n",
    "row = [('month', 'Apple sales', 'Banana sales'),\n",
    "('Jan', 100, 200),\n",
    "('Feb', 230, 111),\n",
    "('Mar', 121, 131),\n",
    "('Apr', 10, 22),\n",
    "('May', 232, 222),\n",
    "('Jun', 231, 222),]\n",
    "\n",
    "for row in rows:\n",
    "    ws.append(row)\n"
   ]
  }
